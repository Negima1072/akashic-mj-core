    "eslint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "test": "jest"
  },
  "author": "Negima1072",
  "contributors": [
    "Satoshi Kobayashi (https://github.com/kobalab)",
